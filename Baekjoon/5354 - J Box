#B3_5354_J Box

t=int(input())
for i in range(t):
  n=int(input())

  for j in range(n):
    if n==1:
      print("#\n")
    elif j==0:
      print("#"*n)
    elif j==n-1:
      print("#"*n)
      if i!=t-1:
        print()
    else:
      print("#",end="")
      print("J"*(n-2),end="")
      print("#")
